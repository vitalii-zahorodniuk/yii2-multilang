{
  "name": "xz1mefx/yii2-multilang",
  "description": "Multilanguage tools package",
  "type": "yii2-extension",
  "keywords": [
    "yii2",
    "locale",
    "lang",
    "language",
    "xZ1mEFx"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Vitalii Zagorodniuk",
      "email": "vitaliizagorodniuk@gmail.com"
    }
  ],
  "require": {
    "yiisoft/yii2": "~2.0"
  },
  "autoload": {
    "psr-4": {
      "xz1mefx\\multilang\\": ""
    }
  },
  "scripts": {
    "post-package-install": [
      "xz1mefx\\multilang\\MultilangMigration::install"
    ],
    "post-package-uninstall": [
      "xz1mefx\\multilang\\MultilangMigration::uninstall"
    ]
  }
}
